{
    "name": "nocs/laravel-retriever",
    "description": "Retrieve cached data made simple.",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alan Schatteman",
            "email": "alan@nineoclocksomewhe.re",
            "homepage": "https://nineoclocksomewhe.re"
        }
    ],
    "homepage": "https://github.com/nineoclocksomewhere/laravel-retriever",
    "keywords": [
        "Laravel", "Cache", "Retriever", "Cached Data", "LaravelRetriever"
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/support": "~7|~8|~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Nocs\\Retriever\\": "src/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nocs\\Retriever\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nocs\\Retriever\\Providers\\RetrieverServiceProvider"
            ]
        }
    }
}
